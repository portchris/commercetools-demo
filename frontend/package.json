{
  "name": "demo-001",
  "version": "1.0.0",
  "description": "A Demo of commercetools + terraform + frontastic",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "next lint -- --ignore-path .gitignore",
    "lint:fix": "pnpm run format && next lint -- --fix --ignore-path .gitignore",
    "sanity:import": "env-cmd -f ../.env sanity dataset import $NEXT_PUBLIC_SANITY_DATASET_RELPATH $NEXT_PUBLIC_SANITY_DATASET --missing",
    "sanity:export": "sanity dataset export production ./data/production.tar.gz",
    "sanity:deploy": "env-cmd -f ../.env sanity deploy",
    "cl": "commercelayer $@",
    "cl:help": "commercelayer help",
    "cl:resources": "commercelayer resources $@",
    "cl:resources:help": "commercelayer resources --help",
    "cl:app:switch -a ": "commercelayer app:switch -a $@",
    "cl:plugin:install": "commercelayer plugins:install $@",
    "cl:logout": "commercelayer logout",
    "cl:checkout": "commercelayer checkout $@ --sku=TSHIRTMS000000FFFFFFLXXX --open",
    "cl:login": "env-cmd -f ../.env commercelayer login -i $NEXT_PUBLIC_CL_CLIENT_ID -s $NEXT_PUBLIC_CL_CLIENT_SECRET -o $NEXT_PUBLIC_CL_ENDPOINT -a $NEXT_PUBLIC_CL_ADMIN",
    "cl:login:customer": "env-cmd -f ../.env commercelayer app:login -i $NEXT_PUBLIC_CL_SALES_CHANNEL_ID -o $NEXT_PUBLIC_CL_ENDPOINT -a $NEXT_PUBLIC_CL_SALES_CHANNEL -S market:code:US"
  },
  "keywords": [
    "commercetools",
    "demo",
    "terraform",
    "commercelayer",
    "sanity",
    "reactjs",
    "nextjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api"
  ],
  "author": "Chris Rogers",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/portchris/commercetools-demo/issues"
  },
  "homepage": "https://github.com/portchris/commercetools-demo#readme",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/git",
    [
      "@semantic-release/github",
      {
        "addReleases": "top"
      }
    ]
  ],
  "dependencies": {
    "@commercelayer/cli": "^6.2.2",
    "@commercelayer/js-auth": "^4.2.1",
    "@commercelayer/react-components": "4.9.0",
    "@headlessui/react": "^1.5.0",
    "@next/env": "^14.1.0",
    "@portabletext/react": "^3.0.4",
    "@sanity/client": "^6.12.4",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.4.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/typescript-estree": "^7.0.1",
    "autoprefixer": "10.4.5",
    "axios": "^1.6.7",
    "iframe-resizer-react": "^1.1.0",
    "js-cookie": "^3.0.1",
    "next": "^14.1.0",
    "next-sanity": "^8.0.0",
    "postcss": "^8.4.35",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "sanity": "^3.28.0",
    "semver": "^7.6.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^20.11.17",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "globby": "^13.1.3",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "semantic-release": "^23.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=10.4.0"
  }
}